<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
    <bean id="cf" class="com.springcore.factory_instance.CarFactory">
    <property name="carname" value="com.springcore.factory_instance.Skoda"/>
    </bean>
<bean id="c" factory-bean="cf" factory-method="getInstance"></bean>
</beans>